# Calculator in Codex

func add(a, b) {
    return a + b
}

func subtract(a, b) {
    return a - b
}

func multiply(a, b) {
    return a * b
}

func divide(a, b) {
    return a / b
}

# Test the calculator
let x = 10
let y = 5

print("Addition:")
print(add(x, y))

print("Subtraction:")
print(subtract(x, y))

print("Multiplication:")
print(multiply(x, y))

print("Division:")
print(divide(x, y))
